extend type Query {
    # 메뉴 항목을 가져와요.
    # fetch menu items
    menus: [Menu]
}

# 메뉴 항목의 구조
# structure of menu item
type Menu {
    id: ID!
    name: String
    uri: String
    upperId: Int
    description: String
    icon: String
    badge: String
    classes: String
    inactive: Boolean
    use: Boolean
    branches: [Menu]
}
